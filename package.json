    "@babel/runtime-corejs3": "^7.4.0",
    "archiver": "^3.0.0",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^4.5.3",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "ejs": "^3.1.7",
    "file-loader": "^1.1.11",
    "mini-css-extract-plugin": "^0.4.4",
    "prettier": "^2.0.5",
    "sass": "^1.52.1",
    "sass-loader": "^10.1.1",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.6.14",
    "web-ext-types": "^2.1.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-extension-reloader": "^1.1.0"
