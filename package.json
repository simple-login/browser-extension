    "@sentry/browser": "^5.25.0",
    "@sentry/integrations": "^5.30.0",
    "axios": "^0.21.3",
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "^2.21.2",
    "tippy.js": "^6.3.2",
    "v-clipboard": "^2.2.3",
    "vue": "^2.6.14",
    "vue-js-modal": "^1.3.35",
    "vue-js-toggle-button": "^1.3.3",
    "vue-router": "^3.5.2",
    "vue-textarea-autosize": "^1.1.1",
    "vue-toasted": "^1.1.28",
    "webextension-polyfill": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
